{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    },
    "userEmail": {
      "type": "email"
    },
    "paymentStatus": {
      "type": "string",
      "required": true
    },
    "isApprove": {
      "type": "boolean",
      "required": true
    },
    "paymentMethod": {
      "type": "string"
    },
    "date": {
      "required": true,
      "type": "datetime"
    },
    "optimumtime": {
      "type": "date"
    }
  }
}
